{
  "manifest_version": 3,
  "name": "Table Cleaner",
  "description": "A Chrome extension to clean the last cell of tables",
  "version": "1.0",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Clean tab",
    "default_popup": "popup.html"
  }
}
